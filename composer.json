{
  "name": "sandro-live/advanced-super-functions",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "Advanced Super Functions is a WordPress Plugin that helps you code quicker and easier.\n\n",
  "homepage": "https://sandro.live/advanced-super-functions/",
  "authors": [
    {
      "name": "Sandro WÃ¼rmli",
      "email": "sandro@wuermli.email",
      "homepage": "https://github.com/sandrowuermli"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": "^7.4|^8.0"
  },
  "require-dev": {
    "composer/installers": "^2.2.0",
    "squizlabs/php_codesniffer": "3.7.1"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "lint": [
      "phpcs --extensions=php --standard=PSR12 app"
    ],
    "php-check": [
      "php --version",
      "which php"
    ]
  }
}
